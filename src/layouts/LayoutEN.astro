---
import Layout from "../layouts/Layout.astro";
export interface Props {
  headerPosition?: "fixed" | "sticky";
}

const props = Astro.props;
---

<Layout
  lang="en"
  headerPosition={props.headerPosition}
  links={[
    {
      name: "Home",
      href: "/en",
      prefetch: true,
    },
    {
      name: "About us",
      href: "/en/about-us",
      prefetch: true,
    },
    {
      name: "Projects",
      href: "/en/projects",
      prefetch: true,
    },
    {
      name: "Blog",
      href: "/en/blog",
      prefetch: true,
    },
    {
      name: "Contacts",
      href: "/en/contacts",
      prefetch: true,
    },
    {
      name: "SK",
      href: "/",
    },
  ]}
>
  <slot />
</Layout>
